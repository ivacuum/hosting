{
  "env": {
    "browser": true
  },
  "extends": "airbnb-base",
  "globals": {
    "$": true,
    "App": true,
    "axios": true,
    "Mousetrap": true,
    "notie": true,
    "Vue": true,
    "VueI18n": true,
    "VueRouter": true,
    "Vuex": true
  },
  "rules": {
    "no-param-reassign": [
      "error",
      {
        "props": true,
        "ignorePropertyModificationsFor": [
          "el",
          "vm",
          "state"
        ]
      }
    ],
    "semi": [
      "error",
      "never"
    ]
  }
}
