{
  "name": "ivacuum/hosting",
  "description": "",
  "license": "MIT",
  "require": {
    "php": ">=8.2",
    "ext-intl": "*",
    "ext-swoole": "*",
    "foolz/sphinxql-query-builder": "^3.0",
    "geo-io/wkb-parser": "^1.0",
    "guzzlehttp/guzzle": "^7.0",
    "http-interop/http-factory-guzzle": "^1.2",
    "ivacuum/generic": "^25.0",
    "laravel/fortify": "^1.8",
    "laravel/framework": "^10.0",
    "laravel/octane": "^2.0",
    "laravel/scout": "^10.0",
    "laravel/socialite": "^5.0",
    "league/commonmark": "^2.2",
    "league/flysystem-sftp-v3": "^3.0",
    "livewire/livewire": "^2.0",
    "meilisearch/meilisearch-php": "^1.0",
    "predis/predis": "^2.0",
    "symfony/css-selector": "^6.0",
    "symfony/dom-crawler": "^6.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.0",
    "barryvdh/laravel-ide-helper": "^2.1",
    "brianium/paratest": "^7.0",
    "fakerphp/faker": "^1.10",
    "laravel/pint": "^1.0",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^7.0",
    "phpunit/phpunit": "^10.0",
    "rector/rector": "^0.17",
    "spatie/laravel-ignition": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "fresh": "@php artisan migrate:fresh --seed --seeder='App\\Seeder\\DatabaseSeeder' --ansi",
    "outdated-direct": "composer outdated --direct",
    "phpcs": "phpcs --standard=PSR12",
    "pint": "vendor/bin/pint --test -v",
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
    ],
    "post-update-cmd": [
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta"
    ],
    "queue": "@php artisan queue:work --stop-when-empty --ansi",
    "rector": "vendor/bin/rector process app tests --dry-run",
    "share": "expose",
    "test": "@php artisan test --parallel",
    "test-linear": "vendor/bin/phpunit --colors=always",
    "test-profile": "@php artisan test --profile",
    "test-recreate-db": "@php artisan test --parallel --recreate-databases"
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "barryvdh/laravel-debugbar",
        "grimzy/laravel-mysql-spatial",
        "laravel/fortify",
        "laravel/telescope"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "php-http/discovery": true
    }
  }
}
