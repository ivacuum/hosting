{
  "preset": "laravel",
  "rules": {
    "blank_line_after_opening_tag": false,
    "braces": {
      "position_after_anonymous_constructs": "same"
    },
    "braces_position": {
      "anonymous_classes_opening_brace": "same_line"
    },
    "class_attributes_separation": {
      "elements": {
        "method": "one"
      }
    },
    "concat_space": {
      "spacing": "one"
    },
    "linebreak_after_opening_tag": false,
    "not_operator_with_successor_space": false,
    "nullable_type_declaration": {
      "syntax": "union"
    },
    "ordered_class_elements": {
      "order": [
        "use_trait",
        "case",
        "constant",
        "property_static",
        "property_public",
        "property_protected",
        "property_private",
        "construct",
        "destruct",
        "magic",
        "phpunit",
        "method_abstract",
        "method_public",
        "method_protected",
        "method_private"
      ]
    },
    "ordered_types": {
      "sort_algorithm": "none",
      "null_adjustment": "always_last"
    },
    "trailing_comma_in_multiline": {
      "elements": [
        "arrays",
        "match",
        "parameters"
      ]
    }
  }
}
